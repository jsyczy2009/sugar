{
  "name": "sugar",
  "version": "1.2.0",
  "description": "A lightweight JavaScript framework (mvvm & template) for building modular Front-end UI components.",
  "keywords": [
    "framework",
    "mvvm",
    "template",
    "modular",
    "frontend",
    "component"
  ],
  "main": "dist/sugar.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tangbc/sugar.git"
  },
  "bugs": "https://github.com/tangbc/sugar/issues",
  "scripts": {
    "pack-mvvm": "webpack --config build/webpack.mvvm.config.js",
    "pack-mvvm-uglify": "webpack --config build/webpack.mvvm.min.config.js",
    "pack-sugar": "webpack --config build/webpack.sugar.config.js",
    "pack-sugar-uglify": "webpack --config build/webpack.sugar.min.config.js",
    "pack-all": "npm run pack-sugar & npm run pack-sugar-uglify & npm run pack-mvvm & npm run pack-mvvm-uglify"
  },
  "author": "TANG",
  "license": "MIT",
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-jshint": "^1.11.2",
    "rollup": "^0.25.6",
    "uglify-js": "^2.6.2",
    "webpack": "^1.12.14"
  }
}
